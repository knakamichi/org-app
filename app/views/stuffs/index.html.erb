<p id="notice"><%= notice %></p>

<h1>Stuffs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sub name1</th>
      <th>Sub name2</th>
      <th>Description1</th>
      <th>Description2</th>
      <th>Description3</th>
      <th>User</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stuffs.each do |stuff| %>
      <tr>
        <td><%= stuff.name %></td>
        <td><%= stuff.sub_name1 %></td>
        <td><%= stuff.sub_name2 %></td>
        <td><%= stuff.description1 %></td>
        <td><%= stuff.description2 %></td>
        <td><%= stuff.description3 %></td>
        <td><%= stuff.user_id %></td>
        <td><%= stuff.image %></td>
        <td><%= link_to 'Show', stuff %></td>
        <td><%= link_to 'Edit', edit_stuff_path(stuff) %></td>
        <td><%= link_to 'Destroy', stuff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stuff', new_stuff_path %>
